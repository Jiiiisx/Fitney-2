{
  "name": "fitney",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:setup": "psql $POSTGRES_URL -f scripts/schema.sql",
    "db:reset": "psql $POSTGRES_URL -c 'DROP TABLE IF EXISTS program_day_exercises, program_days, workout_programs, user_active_plans, exercises, categories, api_exercises_staging, workout_logs CASCADE;'",
    "db:seed-exercises": "ts-node -r dotenv/config -r tsconfig-paths/register scripts/seed-all-exercises.ts",
    "db:seed-programs": "ts-node -r dotenv/config -r tsconfig-paths/register scripts/seed-programs.ts",
    "db:seed": "npm run db:reset && npm run db:setup && npm run db:seed-exercises && npm run db:seed-programs",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dialog": "^1.1.15",
